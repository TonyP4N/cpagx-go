  "safety_interlocks": {
    "Emergency_Shutdown": {
      "triggers": ["LIT101_low", "LIT301_high", "PIT501_high", "UV401_failure"],
      "actions": ["stop_all_pumps", "close_all_valves", "activate_alarm"]
    },
    "Chemical_Safety": {
      "triggers": ["AIT201_out_of_range", "AIT202_out_of_range", "AIT203_out_of_range"],
      "actions": ["stop_chemical_pumps", "close_chemical_valves", "alert_operator"]
    },
    "Membrane_Protection": {
      "triggers": ["DPIT301_high", "PIT501_low"],
      "actions": ["reduce_feed_pressure", "initiate_backwash", "alert_maintenance"]
    }
  }
